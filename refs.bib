    keywords = {gabidulin, network\_coding},
    month = aug,
    number = {8},
    pages = {3579-3591},
    posted-at = {2011-05-18 22:36:50},
    priority = {2},
    title = {Coding for Errors and Erasures in Random Network Coding},
