    keywords = {elliptic\_curve, pairing},
    month = sep,
    posted-at = {2012-10-01 13:28:13},
    priority = {0},
    title = {Faster Pairing Computation on {J}acobi quartic Curves with High-Degree Twists},
    url = {http://eprint.iacr.org/2012/551},
    year = {2012}
